{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "quotemark": [true, "single", "jsx-double"],
    "arrow-parens": false,
    "semicolon": [true, "always", "ignore-bound-class-methods"],
    "trailing-comma": false,
    "object-literal-sort-keys": false,
    "interface-name": false,
    "no-console": false
  },
  "rulesDirectory": []
}
